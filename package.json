{
  "name": "typescript-action",
  "description": "GitHub Actions TypeScript template",
  "version": "v0.5.1",
  "author": "",
  "private": true,
  "homepage": "https://github.com/actions/typescript-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "type": "module",
  "bugs": {
    "url": "https://github.com/actions/typescript-action/issues"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . -c eslint.config.mjs",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "test": "vitest --coverage",
    "test-local": "vitest src/main.test.ts -t 'should run successfully' --hideSkippedTests --reporter=dot",
    "all": "npm run format:write && npm run lint && npm run test -- --run && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^21.1.1",
    "@octokit/webhooks-types": "^7.6.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.200.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "ts-retry": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@octokit/types": "^13.5.0",
    "@opentelemetry/sdk-node": "0.200.0",
    "@opentelemetry/semantic-conventions": "^1.32.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vercel/ncc": "^0.38.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/eslint-plugin": "^1.1.43",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-github": "^5.1.7",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.14.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.1",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vitest": "^3.0.5"
  }
}
